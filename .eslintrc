{
  "parser": "babel-eslint",
  "plugins": [],
  "env": {
    "es6": true,
    "node": true,
    "mocha": true,
    "browser": true,
    "commonjs": true
  },
  "parserOptions": {
    "ecmaVersion": 6
  },
  "rules": {
    "indent": [
      2,
      2
    ],
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "never"
    ],
    "no-console": [
      1
    ]
  }
}
